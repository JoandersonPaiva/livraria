<div class="container">
    <% books.forEach(book => { %>
        <div class="card" style="width: 18rem;">
            <img class="card-img-top" src="/images/books/<%=book.cod%>.jpg" alt="Card image cap">
            <div class="card-body">
              <h5 class="card-title"><%=book.name %></h5>
              <p class="card-text"><%= book.level %></p>
              <a href="/my-books/<%= book.slug%>" class="btn btn-primary">Ler mais</a>
            </div>
          </div>
    <% }) %>
</div>